    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "testold": "react-scripts test",
        "test": "react-scripts test --watchAll=false  --env=jest-environment-jsdom-sixteen",
        "testlive": "react-scripts test --watchAll=true",
        "eject": "react-scripts eject",
        "storybook": "start-storybook -p 9009 -s public",
        "build-storybook": "build-storybook -s public",
        "generate": "plop",
        "sonar": "sonar-scanner",
        "coverage": "npm run test -- --coverage --reporters=default --reporters=jest-junit --watchAll=false"
    },
    "jest-junit": {
        "outputDirectory": "coverage"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/app/**/*.{js,jsx,ts,tsx}",
            "!<rootDir>/node_modules/",
            "!<rootDir>/src/index.tsx",
            "!<rootDir>/src/app/app.container.tsx",
            "!<rootDir>/src/app/app.render.tsx",
            "!<rootDir>/src/app/serviceWorker.ts"
        ],
        "coverageReporters": [
            "text",
            "lcov",
            "cobertura"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@storybook/addon-actions": "^5.3.18",
        "@storybook/addon-links": "^5.3.18",
        "@storybook/addons": "^5.3.18",
